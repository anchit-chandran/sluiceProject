openapi: 3.0.3
info:
  title: Sluice Inventory
  version: 0.0.0
paths:
  /detail/{id}:
    get:
      operationId: detail_retrieve
      parameters:
      - in: path
        name: id
        schema:
          type: integer
        required: true
      tags:
      - detail
      security:
      - cookieAuth: []
      - basicAuth: []
      - {}
      responses:
        '200':
          description: No response body
    put:
      operationId: detail_update
      parameters:
      - in: path
        name: id
        schema:
          type: integer
        required: true
      tags:
      - detail
      security:
      - cookieAuth: []
      - basicAuth: []
      - {}
